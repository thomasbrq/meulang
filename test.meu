function hello(n) {
    if (n == 2) {
        print("n == 2");
    }

    if (n > 2) {
        print("n > 2");
    }

    if (n >= 2) {
        print("n >= 2");
    }

    if (n < 2) {
        print("n < 2");
    }

    if (n <= 2) {
        print("n <= 2");
    }
}

hello(1);
print();
hello(2);
print();
hello(3);
