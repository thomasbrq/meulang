function hello(n) {
    if (n == 2) {
        print("n == 2");
    }

    if (n > 2) {
        print("n > 2");
    }

    if (n >= 2) {
        print("n >= 2");
    }

    if (n < 2) {
        print("n < 2");
    }

    if (n <= 2) {
        print("n <= 2");
    }

    if (n != 2) {
        print("n != 2");
    }
}

print("1:");
hello(1);
print();
print("2:");
hello(2);
print();
print("3:");
hello(3);
